		String product1="Computer";
		String product2 = "Officer Desk";
		
			int age=30;
			int code= 5290;
			char gender = 'F';
			
			double	price1=2100.0;
			double	price2=650.0;
			double measure=53.234567;
			
			System.out.printf("Produtos: \n Computador, cujo preço é R$ %2f",price1);
